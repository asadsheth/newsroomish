astemaker.","published_at":1524018468,"content_id":"6ca18b9b-84c0-3130-be9b-37491bf1b900","content_url":"https:\/\/www.wired.com\/story\/mark-zuckerberg-plays-the-scapegoat-for-our-facebook-sins","content_published_at":1523990940,"content_time":1523990940,"content_relative_time":27,"reaction_count":0,"vibe_name":"Mark Zuckerberg","vibe_id":"6c8abf02-42df-3577-8d9e-f96069c9e05a","content_type":"link","aspect":null}]